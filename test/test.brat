include :file
include :assert

[:examples :containers :function :misc :number :parser :regex :string].each { name |
		true? file.exists?("test/test-#{name}.lua")
			{ file.delete "test/test-#{name}.lua" }

		protect {	p "Loading #{name} tests..."; import "test/test-#{name}" } rescue: { e | p e }
		p "-" * 50
	}
